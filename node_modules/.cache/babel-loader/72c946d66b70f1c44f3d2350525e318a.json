{"ast":null,"code":"import { mapState, mapActions } from 'pinia';\nimport taskStore from '@/store/task';\nimport TaskTemplate from '@/components/TaskTemplate.vue';\nexport default {\n  name: 'HomeView',\n\n  data() {\n    return {\n      showOnlyCompleted: false\n    };\n  },\n\n  computed: { ...mapState(taskStore, ['tasks', 'onlyCompleted', 'onlyPending'])\n  },\n  methods: { ...mapActions(taskStore, ['fetchTasks', 'deleteTask'])\n  },\n\n  created() {\n    this.fetchTasks();\n  },\n\n  components: {\n    TaskTemplate\n  }\n};","map":{"version":3,"mappings":"AAqCA,SAASA,QAAT,EAAmBC,UAAnB,QAAqC,OAArC;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,OAAOC,YAAP,MAAyB,+BAAzB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,iBAAiB,EAAE;IADd,CAAP;EAGD,CANY;;EAObC,QAAQ,EAAE,EACR,GAAGP,QAAQ,CAACE,SAAD,EAAY,CAAC,OAAD,EAAU,eAAV,EAA2B,aAA3B,CAAZ;EADH,CAPG;EAUbM,OAAO,EAAE,EACP,GAAGP,UAAU,CAACC,SAAD,EAAY,CAAC,YAAD,EAAe,YAAf,CAAZ;EADN,CAVI;;EAabO,OAAO,GAAG;IACR,KAAKC,UAAL;EACD,CAfY;;EAgBbC,UAAU,EAAE;IAAER;EAAF;AAhBC,CAAf","names":["mapState","mapActions","taskStore","TaskTemplate","name","data","showOnlyCompleted","computed","methods","created","fetchTasks","components"],"sourceRoot":"","sources":["/Users/maegallego/Desktop/IRONHACK/clonedproject/ToDoApp/to-do-list/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class='home margin' id='home'>\n    <h1>Your list of tasks.</h1>\n    <div v-if='this.tasks.length === 0'>\n      <p>No tasks yet!</p>\n      <router-link class='transparent-btn centered-div' to='/new-task'>Add a new task</router-link>\n    </div>\n    <div class='grid-2' v-if='this.tasks.length'>\n      <div>\n        <h2>Pending tasks</h2>\n        <div v-if='this.onlyPending.length === 0'>\n          <p>You've completed all your tasks.</p>\n        </div>\n        <div v-for='task in onlyPending' :key='task.id' :id='task.id'>\n          <TaskTemplate :taskProp='task' />\n        </div>\n      </div>\n      <div>\n        <h2>Completed tasks</h2>\n        <div v-if='this.onlyCompleted.length === 0'>\n          <p>No tasks completed yet.</p>\n        </div>\n        <div v-for='task in onlyCompleted' :key='task.id' :id='task.id'>\n          <TaskTemplate :taskProp='task' />\n        </div>\n      </div>\n    </div>\n    <div v-if='this.tasks.length'>\n      <router-link class='transparent-btn centered-div' to='/#home'>\n        <span class='material-icons'> arrow_upward </span> Return to\n        top</router-link\n      >\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions } from 'pinia';\nimport taskStore from '@/store/task';\nimport TaskTemplate from '@/components/TaskTemplate.vue';\n\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      showOnlyCompleted: false,\n    };\n  },\n  computed: {\n    ...mapState(taskStore, ['tasks', 'onlyCompleted', 'onlyPending']),\n  },\n  methods: {\n    ...mapActions(taskStore, ['fetchTasks', 'deleteTask']),\n  },\n  created() {\n    this.fetchTasks();\n  },\n  components: { TaskTemplate },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}